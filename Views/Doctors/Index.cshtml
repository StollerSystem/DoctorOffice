@{
  Layout = "_Layout";
}
@using DoctorOffice.Models;
<div class="container">
  <div class="jumbotron">
    <h3>Doctors</h3>
  </div>
  @if(Model.Count == 0)
  {
    <h6>There are no Doctors at this time.</h6>
  }
  <ol>
    @foreach(Doctor doctor in Model)
    {
      <li>@Html.ActionLink(doctor.Name,"Details", new {id = doctor.DoctorId})</li>
    }
  </ol>

  <p>@Html.ActionLink("Create Doctor","Create")</p>
  <p>@Html.ActionLink("Home","Index","Home")</p>



</div>