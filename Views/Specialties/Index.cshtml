@{
  Layout = "_Layout";
}
@using DoctorOffice.Models;
<div class="container">
  <div class="jumbotron">
    <h3>Specialties</h3>
  </div>
  @if(Model.Count == 0)
  {
    <h6>There are no Specialists at this time.</h6>
  }
  <ol>
    @foreach(Specialty specialty in Model)
    {
      <li>@Html.ActionLink(specialty.SpecialtyType,"Details",new {id = specialty.SpecialtyId})</li>
    }
  </ol>

  <p>@Html.ActionLink("Create Specialty","Create")</p>
  <p>@Html.ActionLink("Home","Index","Home")</p>



</div>